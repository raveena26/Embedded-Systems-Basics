#include<avr/io.h>
#include<util/delay.h>
void main()
{
DDRB=0XFF;
while(1)
{
for(int i=1;i<=128;i=i*2)
{
PORTB=i;
_delay_ms(500);
}
PORTB=0x81;
_delay_ms(500);
PORTB=0X82;
_delay_ms(500);
PORTB=0X84;
_delay_ms(500);
PORTB=0X88;
_delay_ms(500);
PORTB=0x90;
_delay_ms(500);
PORTB=0XA0;
_delay_ms(500);
PORTB=0XC0;
_delay_ms(500);
PORTB=0XC1;
_delay_ms(500);
PORTB=0XC2;
_delay_ms(500);
PORTB=0XC4;
_delay_ms(500);
PORTB=0XC8;
_delay_ms(500);
PORTB=0XD0;
_delay_ms(500);
PORTB=0XE0;
_delay_ms(500);
PORTB=0XE1;
_delay_ms(500);
PORTB=0XE2;
_delay_ms(500);
PORTB=0XE4;
_delay_ms(500);
PORTB=0XE8;
_delay_ms(500);
PORTB=0XF0;
_delay_ms(500);
PORTB=0XF1;
_delay_ms(500);
PORTB=0XF2;
_delay_ms(500);
PORTB=0XF4;
_delay_ms(500);
PORTB=0XF8;
_delay_ms(500);
PORTB=0XF9;
_delay_ms(500);
PORTB=0XFA;
_delay_ms(500);
PORTB=0XFC;
_delay_ms(500);
PORTB=0XFD;
_delay_ms(500);
PORTB=0XFE;
_delay_ms(500);
PORTB=0XFF;
_delay_ms(500);


}
}
